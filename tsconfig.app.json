{
  "include": [
    "src/custom.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "strictNullChecks": false,
    "target": "es2015",
    "strict": false,
    "paths": {
      "@services/*": [
        "./services/*"
      ],
      "@libraries/*": [
        "./components/shared/library/*"
      ],
      "@components/*": [
        "./components/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
